{
	"name":"Youtube Notication",
	"version":"1.0",
	"description":"Notifica sobre nuevos videos",
	"manifest_vesion": 2,
	"author":"jonatha",
	"icons":{
		"16":"src/icon16.png",
		"48":"src/icon48.png",
		"128":"src/icon128.png"
	},
	"browser_action":{
		"default_title":"Youtube Notication",
		"default_icon":"src/icon48.png",
		"default_popup":"popup.html"
	},
	"background":{
		"page":"src/background.html"
	},
	"permissions":[
		"notications",
		"storage",
		"tabs",
		"http://*/*",
		"httpS://*/*",
		"cookies",
		"http://www.youtube.com/*"
	],
	"web_accesible_resource":[
		"src/icon48.png"
	]

}